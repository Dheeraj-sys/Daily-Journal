<%- include('partials/header'); -%>
    <h1>Compose</h1>
    <form action="/compose" method="post">
      <div class="form-group">
      <label for="composed">Title</label>
      <input class="form-control" type="text" name="composed" placeholder="enter title">

      <label for="postContent">Post</label>
      <textarea class="form-control" name="postContent" rows="8" cols="80"></textarea>

      <button class="btn btn-primary btn-lg active" aria-pressed="true" type="submit" name="Clicked">Publish</button>
      </div>
    </form>
<%- include('partials/footer'); -%>
